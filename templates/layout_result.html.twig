<script>
    
    
    initiDataTable("#vote-table-mme") ;
    initiDataTable("#vote-table-mr") ;

    exportDatatablePDF();
    function exportDatatablePDF(){
        $('.btn-export-pdf').unbind('click').bind('click', function(){
            var aria = $(this).data('aria') ;
            
            $('button[aria-controls="'+aria+'"]').trigger('click');
        }) ;
    }

    /**
    * Init datatable
    **/
    function initiDataTable(_element){
        

        $(_element).DataTable({
                
            order: [[2, 'desc']],
            dom: 'Bfrtip', 
            //buttons: [ 'copyHtml5', 'excelHtml5', 'csvHtml5', 'pdfHtml5' ] 
            dom: '<"card-header border-bottom p-1"<"head-label"><"dt-action-buttons text-end"B>><"d-flex justify-content-between align-items-center mx-0 row"<"col-sm-12 col-md-6"l><"col-sm-12 col-md-6"f>>t<"d-flex justify-content-between mx-0 row"<"col-sm-12 col-md-6"i><"col-sm-12 col-md-6"p>>',
            displayLength: 100,
            lengthMenu: [100],
            buttons: [
            {
                extend: 'collection',
                className: 'btn btn-outline-secondary dropdown-toggle me-2',
                text: feather.icons['share'].toSvg({ class: 'font-small-4 me-50' }) + 'Export',
                buttons: [
                    /*
                    {
                    extend: 'csv',
                    text: feather.icons['file-text'].toSvg({ class: 'font-small-4 me-50' }) + 'Csv',
                    className: 'dropdown-item',
                    exportOptions: { columns: [0, 1, 2] }
                    },
                    
                    {
                    extend: 'excel',
                    text: feather.icons['file'].toSvg({ class: 'font-small-4 me-50' }) + 'Excel',
                    className: 'dropdown-item',
                    exportOptions: { columns: [0, 1, 2] }
                    },
                    */
                    {
                        extend: 'pdf',
                        text: feather.icons['clipboard'].toSvg({ class: 'font-small-4 me-50' }) + 'Pdf',
                        //className: 'dropdown-item',
                        exportOptions: { columns: [0, 1, 2] },
                        title: function () { return $(_element+'-title').html(); },
                    },
                    /*
                    {
                    extend: 'copy',
                    text: feather.icons['copy'].toSvg({ class: 'font-small-4 me-50' }) + 'Copy',
                    className: 'dropdown-item',
                    exportOptions: { columns: [0, 1, 2, 3, 4, 5] }
                    }
                    */
                ],
                init: function (api, node, config) {
                    $(node).removeClass('btn-secondary');
                    $(node).parent().removeClass('btn-group');
                    setTimeout(function () {
                    $(node).closest('.dt-buttons').removeClass('btn-group').addClass('d-inline-flex');
                    }, 50);
                }
            },
            
        ]
        }) ;


    }

    $(window).on('load', function () {
        if (feather) {
            feather.replace({
                width: 14,
                height: 14
            });
        }
    })
</script>
